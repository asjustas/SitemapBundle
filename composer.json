{
    "name":         "tadcka/sitemap-bundle",
    "homepage":     "https://github.com/tadcka/SitemapBundle",
    "description":  "Sitemap bundle on Symfony2",
    "keywords":     ["sitemap", "site page management system"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors": [
        {
            "name":   "Tadas Gliaubicas",
            "email":  "tadcka89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",

        "symfony/framework-bundle" : ">=2.3",
        "friendsofsymfony/jsrouting-bundle": "~1.5",
        "nercury/translation-editor-bundle": "~0.1",
        "jms/serializer-bundle": "~1.0",
        "aferrandini/urlizer": "1.0.*",

        "silvestra/seo-bundle": "0.1.*@dev",
        "tadcka/tree": "0.2.*@dev",
        "tadcka/jstree-bundle": "~0.2",
        "tadcka/routing-bundle": "0.2.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "For doctrine mapping.",
        "stof/doctrine-extensions-bundle": "For doctrine mapping.",
        "silvestra/form": "For extra form types."
    },
    "autoload": {
        "psr-4": {
            "Tadcka\\Bundle\\SitemapBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
